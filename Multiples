#include "stdafx.h"
#include "Multiples.h"


Multiples::Multiples()
{
}


Multiples::~Multiples()
{
}

int Multiples::findMultiple(int mult1, int mult2, int cap)
{
	int sum = 0;
	int i = mult1, j = mult2;
	while (cap < 1000)
	{
		if (i % mult1 == 0){
			sum += i;
		}if (j % mult2 == 0){
			sum += j;
		}
		cap++;
	}
	return sum;
}
